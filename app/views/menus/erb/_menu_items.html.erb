	<div class="container">
	  <div class="row">
		<div class="span4">
		  <div class="well sidebar-nav">  
	              <ul class="nav nav-list">  
	                <li class="nav-header">Apetizers</li>  
					<table>
					<%@menu.menu_items.each do |item|%>
						<%if !item.name.blank?%>
						<tr>
							<td><%=item.name%> </td>
							<td>.........      </td>
							<td>$<%=item.price%></td>
							<td><%=link_to " X", 
							"/menu/#{@menu.id}/menu_item/#{item.id}",
			                :method => 'delete',
			                :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?'))
							%></td>
						</tr>
						<%end%>
					<%end%>	
				</table>
	             </ul>  
	   	  </div><!--/.well --> 
    	</div>
	</div>
</div>
