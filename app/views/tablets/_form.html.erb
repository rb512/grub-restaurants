<%= simple_form_for(@tablet) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :serial_id %>
    <%= f.input :restaurant_id %>
    <%= f.input :owner_id %>
    <%= f.input :type %>
    <%= f.input :ip_address %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
